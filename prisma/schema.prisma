generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(uuid())
  fistName  String?
  lastName  String?
  email     String    @unique
  hash      String
  role      UserRoles @default(CLIENT)
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt

  @@map("users")
}

enum UserRoles {
  CLIENT
  ADMIN
}
